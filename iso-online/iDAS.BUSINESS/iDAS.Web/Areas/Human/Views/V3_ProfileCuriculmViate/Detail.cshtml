            X.TextFieldFor(m => m.Bank).ReadOnly(true).FieldLabel("Ngân hàng").MarginSpec("0 8 0 8").Width(740),
            X.TextFieldFor(m => m.NumberOfPassport).ReadOnly(true).FieldLabel("Số hộ chiếu").MarginSpec("0 8 0 8").Width(740),
            X.TextFieldFor(m => m.PlaceOfPassport).ReadOnly(true).FieldLabel("Nơi cấp hộ chiếu").MarginSpec("0 8 0 8").Width(740),
