            X.TextFieldFor(m => m.PosititonParty).ReadOnly(true).FieldLabel("Chức vụ Đảng").MarginSpec("0 8 0 8").Width(740),
            X.TextFieldFor(m => m.NumberOfPartyCard).ReadOnly(true).FieldLabel("Số thẻ Đảng").MarginSpec("0 8 0 8").Width(740),
            X.TextFieldFor(m => m.DateOnArmy).ReadOnly(true).FieldLabel("Ngày nhập ngũ").MarginSpec("0 8 0 8").Width(740),
            X.TextFieldFor(m => m.PositionArmy).ReadOnly(true).FieldLabel("Chức vụ quân đội").MarginSpec("0 8 0 8").Width(740),
