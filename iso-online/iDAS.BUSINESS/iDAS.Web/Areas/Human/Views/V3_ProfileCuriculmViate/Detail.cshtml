            X.TextFieldFor(m => m.PlaceIssueOfIdentityCard).ReadOnly(true).FieldLabel("Nơi cấp CMT").MarginSpec("0 8 0 8").Width(740),
            X.TextFieldFor(m => m.DateOnGroup).ReadOnly(true).FieldLabel("Ngày vào đoàn").MarginSpec("0 8 0 8").Width(740),
            X.TextFieldFor(m => m.PositionGroup).ReadOnly(true).FieldLabel("Chức vụ đoàn").MarginSpec("0 8 0 8").Width(740),
            X.TextFieldFor(m => m.PlaceOfLoadedGroup).ReadOnly(true).FieldLabel("Nơi kết nạp đoàn").MarginSpec("0 8 0 8").Width(740),
            X.TextFieldFor(m => m.DateJoinRevolution).ReadOnly(true).FieldLabel("Ngày tham gia cách mạng").MarginSpec("0 8 0 8").Width(740),
