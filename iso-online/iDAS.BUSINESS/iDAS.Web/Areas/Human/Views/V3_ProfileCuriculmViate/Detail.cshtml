@using Ext.Net;
@using Ext.Net.MVC;
@using iDAS.Web;
@model iDAS.Items.HumanProfileCuriculmViateItem
@{
    var X = Html.X();
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@(Html.X().Viewport().Layout(LayoutType.Fit).Border(false).Margin(0).Frame(false)
 .Items(
    Html.X().FormPanel()
    .Layout(LayoutType.Form)
    .Frame(false)
    .Border(false).AutoScroll(true)
    .FieldDefaults(df => df.LabelWidth = 100)
    .DefaultAnchor("100%")
    .Items(
            X.TextFieldFor(m => m.Aliases).ReadOnly(true).FieldLabel("Bí danh").MarginSpec("0 8 0 8").Width(740),
            X.TextFieldFor(m => m.WeddingStatus).ReadOnly(true).FieldLabel("Tình trạng hôn nhân").MarginSpec("0 8 0 8").Width(740),
            X.TextFieldFor(m => m.Nationality).ReadOnly(true).FieldLabel("Quốc tịch").MarginSpec("0 8 0 8").Width(740),
            X.TextFieldFor(m => m.People).ReadOnly(true).FieldLabel("Dân tộc").MarginSpec("0 8 0 8").Width(740),
            X.TextFieldFor(m => m.Religion).ReadOnly(true).FieldLabel("Tôn giáo").MarginSpec("0 8 0 8").Width(740),
            X.TextFieldFor(m => m.PlaceOfBirth).ReadOnly(true).FieldLabel("Nơi sinh").MarginSpec("0 8 0 8").Width(740),
            X.TextFieldFor(m => m.HomePhone).ReadOnly(true).FieldLabel("Điện thoại cơ quan").MarginSpec("0 8 0 8").Width(740),
            X.TextFieldFor(m => m.OfficePhone).ReadOnly(true).FieldLabel("Điện thoại nhà riêng").MarginSpec("0 8 0 8").Width(740),
            X.TextFieldFor(m => m.NumberOfIdentityCard).ReadOnly(true).FieldLabel("Số chứng minh nhân dân").MarginSpec("0 8 0 8").Width(740),
