@using Ext.Net.MVC;
@using Ext.Net;
@{
    var urlPlanStore = ViewData["UrlPlanStore"].ToString();
    var urlTargetStore = ViewData["UrlTargetStore"].ToString();
    var urlSubmit = ViewData["UrlSubmit"].ToString();
    var paramID = ViewData["ParamID"].ToString();
}
@(Html.X().Window().ID("PlanWindowViewID")
    .Title("Danh sách kế hoạch")
    .Icon(Icon.ApplicationSideList)
    .Height(1)
    .Maximized(true)
    .Constrain(true)
    .Layout(LayoutType.Fit)
    .Modal(true)
    .Resizable(false)
    .BodyPadding(0)
    .Border(true)
    .Center()
        .ItemsFromAction("PlanListView", "Plan",
                new { area = "Quality", urlPlanStore = urlPlanStore, urlTargetStore = urlTargetStore, urlSubmit = urlSubmit, paramID = paramID })
)