@using Ext.Net.MVC;
@using Ext.Net;
@{
    var urlStore =  ViewData["UrlStore"].ToString();
    var paramID =  ViewData["ParamID"].ToString();
}
@(Html.X().Window().ID("TargetWindowViewID")
    .Title("Danh sách mục tiêu")
    .Icon(Icon.ApplicationSideList)
    .Height(1)
    .Maximized(true)
    .Constrain(true)
    .Layout(LayoutType.Fit)
    .Modal(true)
    .Resizable(false)
    .BodyPadding(0)
    .Border(true)
    .Center()
    .ItemsFromAction("TargetListView", "Target", new { area = "Quality", urlStore = urlStore, paramID = paramID })
)