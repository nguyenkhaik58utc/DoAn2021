@using Ext.Net.MVC
@using Ext.Net
@{
    var urlStore = ViewData["UrlStore"].ToString();
    var paramID = ViewData["ParamID"].ToString();
}
@(Html.X().Window()
    .ID("WindowNCView")
    .Title("Danh sách điểm không phù hợp")
    .Icon(Icon.AwardStarBronze1)
    .Height(1)
    .Maximized(true)
    .Constrain(true)
    .Layout(LayoutType.Fit)
    .Modal(true)
    .BodyPadding(0)
    .Border(false)
    .StyleSpec("border: none;")
    .Resizable(false)
    .Center()
    .ItemsFromAction("List", "NC", new { area = "Quality", urlStore = urlStore, paramID = paramID })
)

