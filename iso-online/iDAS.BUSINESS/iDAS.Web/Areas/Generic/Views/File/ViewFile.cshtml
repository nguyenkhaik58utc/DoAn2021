@using Ext.Net;
@using Ext.Net.MVC;
@model string
@(Html.X().Window().Height(1).AutoScroll(true).Maximized(true)
    .Title("Xem nội dung tệp")
    .BodyStyle("background-color:white")
    .Layout(LayoutType.Fit)
    .AutoScroll(!Model.Contains("application/pdf"))
    .Border(false)
    .Frame(true)
    .Html(a => Model)
)
