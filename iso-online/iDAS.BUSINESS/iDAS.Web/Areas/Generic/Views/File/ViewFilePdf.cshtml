@using Ext.Net;
@using Ext.Net.MVC;
@model string
@(Html.X().Window()
    .Maximized(true)
    .Maximizable(true)
    .Width(400)
    .Height(450)
    .Title("Xem nội dung tệp")
    .Icon(Icon.PageWhiteText)
    .Layout(LayoutType.Fit)
    .Border(false)
    .AutoScroll(false)
    .Frame(true)
    .Modal(true)
    .Html(m => "<iframe src='"+Model+"' width='100%' height='100%'></iframe>")
)
