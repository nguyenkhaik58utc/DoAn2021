@using Ext.Net;
@using Ext.Net.MVC;
@model string
@(Html.X().Window()
    .Height(1)
    .Maximized(true)
    .Icon(Icon.BookOpen)
    .Title("Hướng dẫn sử dụng")
    .BodyStyle("background-color:white")
    .Layout(LayoutType.Fit)
    .AutoScroll(!Model.Contains("application/pdf"))
    .Border(false)
    .Frame(false)
    .Html(a=>Model)
)
